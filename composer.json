{
    "name": "websvc/yii2-multiple-input",
    "description": "Widget for handle multiple inputs for an attribute of Yii2 framework model",
    "keywords": [
        "yii2",
        "yii2 multiple input",
        "yii2 array input",
        "yii2 multiple field",
        "yii2 tabular input"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/websvcPT/yii2-multiple-input/issues?state=open",
        "source": "https://github.com/websvcPT/yii2-multiple-input"
    },
    "authors": [
        {
            "name": "Eugene Tupikov",
            "email": "unclead.nsk@gmail.com"
        },
        {
            "name": "Nelson Dias",
            "homepage": "https://github.com/websvcPT/yii2-multiple-input"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.38"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    },
    "autoload": {
        "psr-4": {
          "websvc\\multipleinput\\examples\\": "examples/",
          "websvc\\multipleinput\\": "src/",
          "websvc\\multipleinput\\tests\\": "tests/"
        }
    }
}
